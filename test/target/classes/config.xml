<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

	<bean class="edu.jsp.test.Number" id="n1" c:id="1"
		c:number="1234567890" c:network="JIO" />
	<bean class="edu.jsp.test.Number" id="n2" c:id="2"
		c:number="1234567890" c:network="JIO" />
	<bean class="edu.jsp.test.Number" id="n3" c:id="3"
		c:number="098654321" c:network="idea" />


	<bean class="edu.jsp.test.BankAcc" id="b1" c:id="1" c:bname="SBI"
		c:accno="500101012204241" c:branch="Avadi">
	</bean>


	<bean class="edu.jsp.test.Vehicle" id="v1" c:id="1" c:name="MT-15"
		c:color="Black" c:number="TN-12 AF-8173" />
	<bean class="edu.jsp.test.Vehicle" id="v2" c:id="2" c:name="R15"
		c:color="Black" c:number="TN-12 AF-8199" />
	<bean class="edu.jsp.test.Vehicle" id="v3" c:id="3"
		c:name="Apache" c:color="Black" c:number="TN-12 AF-8143" />
	<bean class="edu.jsp.test.Vehicle" id="v4" c:id="4"
		c:name="NS-200" c:color="Black" c:number="TN-12 AF-4478" />

	<bean class="edu.jsp.test.Person" id="p1" c:id="1" c:name="raj"
		c:age="22">
		<constructor-arg name="bankAccs">
			<map>
				<entry key="SBI" value-ref="b1" />
			</map>
		</constructor-arg>
		<constructor-arg name="numbers">
			<set>
				<ref bean="n1" />
				<ref bean="n2" />
				<ref bean="n3" />
			</set>
		</constructor-arg>
		<constructor-arg name="vehicles">
			<list>
				<ref bean="v1" />
				<ref bean="v2" />
				<ref bean="v3" />
				<ref bean="v4" />
			</list>
		</constructor-arg>

	</bean>


</beans>